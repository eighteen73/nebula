{
	"name": "nebula-blocks",
	"version": "0.1.0",
	"description": "Custom blocks for a Nebula project.",
	"author": "eighteen73",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"start": "wp-scripts start --experimental-modules",
		"build": "wp-scripts build --experimental-modules",
		"format": "npm run format:css && npm run format:js",
		"lint": "npm run lint:css && npm run lint:js",
		"lint:css": "stylelint './src/**/*.scss'",
		"lint:js": "eslint './src/**/*.js'",
		"format:css": "stylelint --fix './src/**/*.scss'",
		"format:js": "eslint --fix './src/**/*.js'",
		"packages-update": "wp-scripts packages-update",
		"create-block": "npx @wordpress/create-block@latest --template @eighteen73/create-block-template --no-plugin"
	},
	"files": [
		"[^.]*"
	],
	"dependencies": {
		"@eighteen73/eslint-config-wordpress": "^1.0.3",
		"@eighteen73/stylelint-config-wordpress": "^1.1.2"
	},
	"devDependencies": {
		"@wordpress/scripts": "^29.0.0"
	}
}
